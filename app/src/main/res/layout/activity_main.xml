<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tuz.google.MainActivity">


    <Button
        android:id="@+id/sign_google"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="登录google"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/sign_face_book"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="登录facebook"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sign_google"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/google_play"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="google支付"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sign_face_book"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/google_analytics"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="google analytics"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/google_play"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/google_pay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="google pay"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/google_analytics"
        tools:ignore="HardcodedText" />




</androidx.constraintlayout.widget.ConstraintLayout>